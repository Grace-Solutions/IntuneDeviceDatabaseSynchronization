# Microsoft Graph API Configuration
GRAPH_CLIENT_ID=your-azure-client-id-here
GRAPH_CLIENT_SECRET=your-azure-client-secret-here
GRAPH_TENANT_ID=your-azure-tenant-id-here

# Sync Configuration
POLL_INTERVAL=1h
DEVICE_OS_FILTER=Windows,macOS,Android

# Prometheus Configuration
ENABLE_PROMETHEUS=true
PROMETHEUS_PORT=9898
PROMETHEUS_SCRAPE_INTERVAL=0 */30 * * * *

# Database Configuration
MSSQL_CONNECTION_STRING=Server=localhost;Database=IntuneDevices;User Id=sa;Password=YourPassword123!;
MSSQL_TABLE_NAME=devices

# Logging Configuration
RUST_LOG=info
